    "pretest": "npm run lint"
  },
  "author": "",
  "dependencies": {
    "babel": "^6.1.5",
    "babel-cli": "^6.1.4",
    "babel-preset-es2015": "^6.1.4",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "mocha": "^2.3.3"
  },
  "devDependencies": {

  }
}
