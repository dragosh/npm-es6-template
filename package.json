{
  "name": "@backbase/node-module-boilerplate",
  "version": "1.0.0",
  "description": "",
  "jsnext:main": "src/index.js",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "build": "mkdir -p ./dist && browserify  ./src/index.js  -t babelify --outfile ./dist/index.js",
